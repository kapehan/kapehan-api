{
  "version": 2,
  "builds": [
    {
      "src": "init-server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "routes/**"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "init-server.js" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true
    }
  }
}
